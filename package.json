{
  "name": "pixi-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,scss}\" --ignore-path \".prettierignore\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,scss}\" --ignore-path \".prettierignore\"",
    "make-spritesheet": "spritesheet-js --trim --format pixi.js",
    "make-spritesheet:cards": "yarn make-spritesheet --path ./public/assets/cards --name cards ./raw-assets/cards/*.png",
    "make-spritesheet:magicWords": "yarn make-spritesheet --path ./public/assets/magic-words --name magic-words ./raw-assets/magic-words/*.png"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/stats.js": "^0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "spritesheet-js": "^1.2.8",
    "typescript": "~5.6.3",
    "vite": "^5.4.10"
  },
  "packageManager": "yarn@4.5.1",
  "dependencies": {
    "@pixi/core": "^7.4.2",
    "@pixi/display": "^7.4.2",
    "@pixi/filter-alpha": "^7.4.2",
    "@pixi/filter-blur": "^7.4.2",
    "@pixi/graphics": "^7.4.2",
    "@pixi/layout": "^1.0.1",
    "@pixi/mesh": "^7.4.2",
    "@pixi/mesh-extras": "^7.4.2",
    "@pixi/sprite": "^7.4.2",
    "@pixi/sprite-tiling": "^7.4.2",
    "@pixi/text": "^7.4.2",
    "@pixi/ui": "^1.1.3",
    "gsap": "^3.12.5",
    "pixi-filters": "^5.3.0",
    "pixi.js": "^7.4.2",
    "stats.js": "^0.17.0"
  }
}
